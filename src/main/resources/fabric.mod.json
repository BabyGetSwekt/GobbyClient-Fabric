{
	"schemaVersion": 1,
	"id": "gobbyclient",
	"version": "${version}",
	"name": "gobbyclient",
	"description": "i FUCKING hate cheaters!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/gobbyclient/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			{
				"value": "gobby.Gobbyclient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "gobby.GobbyclientDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		{
			"config": "gobbyclient.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}